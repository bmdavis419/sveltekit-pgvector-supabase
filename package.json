    "ai": "^3.1.11",
    "bits-ui": "^0.21.7",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.30.10",
    "postgres": "^3.4.4",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
  }
